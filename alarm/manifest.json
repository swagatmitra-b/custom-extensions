{
    "manifest_version": 3,
    "name": "Alarm",
    "version": "1.0.1",
    "incognito": "split",
    "permissions": [
        "storage",
         "tabs", 
         "activeTab"
    ],
    "background": {
        "service_worker": "background.js", 
        "type": "module"
    },
    "host_permissions": [

    ],
    "content_scripts": [
      {
        "js": ["alarm.js"],
        "matches": ["<all_urls>"]
      }
    ], 
    "action": {
        "default_title": "alarm",
        "default_popup": "popup.html"
    }
  }
  